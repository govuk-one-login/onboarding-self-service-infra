{
  "name": "example-waf-bootstrap",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest tests/"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "esbuild-jest"
    },
    "clearMocks": true,
    "reporters": [
      "default", [ "jest-html-reporter",
        {
          "includeFailureMsg": true,
          "append": true,
          "pageTitle": "example-waf-bootstrap test report",
          "outputPath": "../test-reports/index.html"
        }]
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageProvider": "v8",
    "testMatch": [
      "**/tests/**/*.test.ts",
      "**/*.steps.ts",
      "**/*.step.ts"
    ]
  },
  "devDependencies": {
    "@aws-cdk/assertions": "^1.174.0",
    "@types/jest": "^29.0.3",
    "aws-cdk-lib": "^2.47.0",
    "esbuild": "^0.25.11",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.5.0",
    "jest-cucumber": "^3.0.1",
    "jest-html-reporter": "^3.10.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "yaml-cfn": "^0.3.2"
  },
  "description": "This is a starter configuration for the WAF.",
  "author": "",
  "directories": {
    "test": "tests"
  }
}
